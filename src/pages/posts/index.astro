---
import { getCollection } from "astro:content";

const posts = await getCollection("posts");
---

<ul>
    {
        posts.map((post: { slug: any; data: { title: unknown } }) => (
            <li>
                <a href={`/posts/${post.slug}`}>{post.data.title}</a>
            </li>
        ))
    }
</ul>
